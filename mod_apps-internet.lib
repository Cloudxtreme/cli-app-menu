# +----------------------------------------+
# |    Function f_menu_app_web_browsers    |
# +----------------------------------------+
#
#  Inputs: None. 
#    Uses: MWB, MENU_ITEM, MAX.
# Outputs: ERROR, MENU_TITLE, DELIMITER, PRESS_KEY.
#
f_menu_app_web_browsers () {
      f_initvars_menu_app
      until [ $MWB -eq 0 ]
      do    # Start of Web Browsers Applications until loop.
            #MWB elinks  - Web browser, tables, frames, forms support, tabbed browsing.
            #MWB links   - Web browser, no graphics mode.
            #MWB links2  - Web browser, has graphics mode.
            #MWB lynx    - Web browser, NLS support.
            #MWB retawq  - Web browser, multi-threaded.
            #MWB surfraw - Search the web using a web search site.
            #MWB w3m     - Web browser, tables, frames support, IPv6 support.
            #
            PRESS_KEY=1 # Display "Press 'Enter' key to continue."
            THIS_FILE="mod_apps-internet.lib"
            MENU_TITLE="Web Browser Applications Menu"
            DELIMITER="#MWB" #MWB This 3rd field prevents awk from printing this line into menu options. 
            f_show_menu $MENU_TITLE $DELIMITER 
            #
            read MWB
            #
            f_menu_item_process $MWB  # Outputs $MENU_ITEM.
            f_application_help
            ERROR=0 # Reset error flag.
            APP_NAME="" # Set application name to null value.
            #
            case $MENU_ITEM in # Start of Web Browser Applications case statement.
                 elinks' '*)
                 APP_NAME=$MENU_ITEM
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 [Ee] | [Ee][Ll] | [Ee][Ll][Ii] | [Ee][Ll][Ii][Nn] | [Ee][Ll][Ii][Nn][Kk] | [Ee][Ll][Ii][Nn][Kk][Ss])
                 APP_NAME="elinks"
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 links' '*)
                 APP_NAME=$MENU_ITEM
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 [Ll] | [Ll][Ii] | [Ll][Ii][Nn] | [Ll][Ii][Nn][Kk] | [Ll][Ii][Nn][Kk][Ss] | [Ll][Ii][Nn][Kk][Ss]) 
                 APP_NAME="links"
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 links2' '*)
                 APP_NAME=$MENU_ITEM
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 [Ll] | [Ll][Ii] | [Ll][Ii][Nn] | [Ll][Ii][Nn][Kk] | [Ll][Ii][Nn][Kk][Ss] | [Ll][Ii][Nn][Kk][Ss][2] | [Ll][Ii][Nn][Kk][Ss][2])
                 APP_NAME="links2"
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 lynx' '*)
                 APP_NAME=$MENU_ITEM
                 f_web_site
                 f_application_run
                 ;;
                 [Ll] | [Ll][Yy]*)
                 APP_NAME="lynx"
                 f_web_site
                 f_application_run
                 ;;
                 retawq' '*)
                 APP_NAME=$MENU_ITEM
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 [Rr] | [Rr][Ee] | [Rr][Ee][Tt] | [Rr][Ee][Tt][Aa]*)
                 APP_NAME="retawq"
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 surfraw' '*)
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Ss] | [Ss][Uu]*)
                 APP_NAME="surfraw"
                 f_application_run
                 ;;
                 w3m' '*)
                 APP_NAME=$MENU_ITEM
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
                 [Ww] | [Ww][3]*)
                 APP_NAME="w3m"
                 f_web_site
                 f_how_to_quit_application "q"
                 f_application_run
                 ;;
            esac                # End of Web Browsers Applications case statement.
            PRESS_KEY=0 # Do not display "Press 'Enter' key to continue."
            #
            # Trap bad menu choices, do not echo Press enter key to continue.
            f_bad_menu_choice $MENU_ITEM  # Outputs $MENU_ITEM.
            #
            MWB=$MENU_ITEM
            #
            # If application displays information, allow user to read it.
            f_option_press_enter_key
            #
      done  # End of Web Browsers Applications until loop.
            #
      unset MWB MENU_ITEM  # Throw out this variable.
} # End of function f_menu_app_web_browsers
