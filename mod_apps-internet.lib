                 f_application_run
                 ;;
                 bittornado' '*)
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Bb] | [Bb][Ii] | [Bb][Ii][Tt] | [Bb][Ii][Tt][Tt] | [Bb][Ii][Tt][Tt][Oo] | [Bb][Ii][Tt][Tt][Oo][Rr] | [Bb][Ii][Tt][Tt][Oo][Rr][Nn]*)
                 APP_NAME="bittornado"
                 f_application_run
                 ;;
                 bittorrent' '*)
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Bb] | [Bb][Ii] | [Bb][Ii][Tt] | [Bb][Ii][Tt][Tt] | [Bb][Ii][Tt][Tt][Oo] | [Bb][Ii][Tt][Tt][Oo][Rr] | [Bb][Ii][Tt][Tt][Oo][Rr][Rr]*)
                 APP_NAME="bittorrent"
                 f_application_run
                 ;;
                 ctorrent' '*)
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Cc] | [Cc][Tt]*)
                 APP_NAME="ctorrent"
                 f_application_run
                 ;;
                 deluge' '*)
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Dd] | [Dd][Ee]*)
                 APP_NAME="deluge"
                 f_application_run
                 ;;
                 mldonkey' '*)
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Mm] | [Mm][Ll]*)
                 APP_NAME="mldonkey"
                 f_application_run
                 ;;
                 rtorrent' '*) 
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
                 [Rr] | [Rr][Tt]*)
                 APP_NAME="rtorrent"
                 f_application_run
                 ;;
                 transmission' '*) 
                 APP_NAME=$MENU_ITEM
                 f_application_run
                 ;;
